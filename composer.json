{
    "name": "rockbuzz/lara-https",
    "description": "Configure https when needed.",
    "keywords": ["laravel", "HTTPS", "SSL"],
    "license": "MIT",
    "authors": [
        {
            "name": "TiagoDevWeb",
            "email": "tiago@rockbuzz.com.br"
        },
        {
            "name": "Paulo Henrique P. Scheuermann",
            "email": "paulo@rockbuzz.com.br"
        }
    ],
    "autoload": {
        "psr-4": {
            "Rockbuzz\\LaraHttps\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox",
        "cs": "vendor/bin/phpcs"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rockbuzz\\LaraHttps\\ServiceProvider"
            ]
        }
    }
}